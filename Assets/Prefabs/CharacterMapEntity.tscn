	    vec4 img = texture(TEXTURE, uv + vec2(offs_uv.x, offs_uv.y));
	    COLOR = img;
	}else{
		COLOR =texture(TEXTURE, UV);
	}
    
}

void vertex(){
	VERTEX.g += sin(TIME* 4.0)* 20.0;
